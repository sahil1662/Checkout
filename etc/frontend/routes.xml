<?xml version="1.0" ?>
<!--/**
 * Define module frontend routes
 * PM KL @link https://projects.zoho.com/portal/pixelmechanics2#taskdetail/1781812000000140023/1781812000000179041/1781812000000501041
 */-->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:App/etc/routes.xsd">
    <!--Use router 'standard' for frontend route-->
    <router id="standard">
        <!--Define a custom route with id and frontName-->
        <route frontName="pmcheckout" id="pmcheckout">
            <!--The module which this route match to-->
            <module name="Pixelmechanics_Checkout"/>
        </route>
    </router>
</config>